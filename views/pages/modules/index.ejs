<!DOCTYPE html>
<html lang="en">
<head>
	<% include ./partials/head %>
</head>
<body class="container">
	<header>
		<% include ./partials/header %>
	</header>

	<main>
		<div class="col-lg-12">
	        <ol class="breadcrumb">
	        	<li><a href="#">Metasploit App</a></li>
	        	<li class="active">Modules</li>
	        </ol>
	        <table class="table table-hover">
	        	<thead>
	        		<tr>
		        		<th>Disclosure Date</th>
		        		<th>Name</th>
		        		<th>Rank</th>
		        	</tr>
		        </thead>

		        <tbody>
		        	<tr>
		        		<td><%= module.disclosuredate %></td>
		        		<td>
		        			<a href="/modules/<%= module.fullname %>/new">
		        				<%= module.name %>
		        			</a>
		        		</td>
		        		<td><%= module.rank %></td>
		        	</tr>
	        	</tbody>
	        </table>
        </div>
    </main>

    <footer>
        <% include ./partials/footer %>
    </footer>

</body>
</html>