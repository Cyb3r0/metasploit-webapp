<!DOCTYPE html>
<html lang="en">
<head>
	<% include ./partials/head %>
</head>
<body class="container">
	<header>
		<% include ./partials/header %>
	</header>

	<main>
		<div class="col-lg-12">
	        <ol class="breadcrumb">
	        	<li><a href="#">Metasploit App</a></li>
	        	<li class="active">Sessions</li>
	        </ol>
	        <table class="table table-hover">
	        	<thead>
	        		<tr>
		        		<th>Id</th>
		        		<th>Type</th>
		        		<th>Name</th>
		        		<th>Tunnel</th>
		        		<th>Info</th>
		        	</tr>
		        </thead>

		        <tbody>
		        	<% for(var Id in sessions) { %>
		        	<tr>
		        		<td><%= Id %></td>
		        		<td><%= sessions[Id].type %></td>
		        		<td>
		        			<a href="/sessions/<%= Id %>/post">
		        				<%= sessions[Id].via_exploit %>
		        			</a>
		        		</td>
		        		<td>local:<%= sessions[Id].tunnel_local %>,peer<%= sessions[Id].tunnel_peer %></td>
		        		<td><%= sessions[Id].info %></td>
		        	</tr>
		        	<%	} %>
	        	</tbody>
	        </table>
        </div>
    </main>

    <footer>
        <% include ./partials/footer %>
    </footer>

</body>
</html>